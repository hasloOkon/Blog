@model Blog.WebApp.ViewModels.RightSidebarViewModel

<div id="sidebars">
    <div class="sidebar">
        @Html.Partial("_Search")
    </div>
    <div class="sidebar">
        <div class="text-center section-title">
            <h3>Tagi</h3>
        </div>
        @foreach (var tag in Model.Tags)
        {
            <span  class="mt-2">@Html.TagLink(tag)</span>
        }
    </div>

    <div class="sidebar">
        <div class="text-center section-title">
            <h3>O mnie</h3>
        </div>
        @foreach (var tag in Model.Tags)
        {
            <span>@Html.TagLink(tag)</span>
        }
    </div>

</div>

